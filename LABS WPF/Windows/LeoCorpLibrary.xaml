<Window x:Class="LABS_WPF.Windows.LeoCorpLibrary"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LABS_WPF"
        xmlns:windows="clr-namespace:LABS_WPF.Windows"
        xmlns:extensions="clr-namespace:LABS_WPF.Extensions"
        mc:Ignorable="d"
        Title="LeoCorpLibrary - LABS WPF" Height="331" Width="538" WindowStartupLocation="CenterScreen" Icon="/LABS WPF;component/Logo.png">
    <Grid>
        <StackPanel FlowDirection="LeftToRight" Orientation="Horizontal" VerticalAlignment="Top">
            <local:TestButton Content="Sum" extensions:CornerRadiusExtension.CornerRadius="5" Height="34" Width="102" Click="TestButton_Click"></local:TestButton>
            <local:TestButton Content="DPI/Scale" extensions:CornerRadiusExtension.CornerRadius="5" Height="34" Width="102" Click="ScaleBtn_Click" x:Name="ScaleBtn"></local:TestButton>
        </StackPanel>
    </Grid>
</Window>
